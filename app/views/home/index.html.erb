<script>
$(document).ready(function(){
    $('.modal-trigger').leanModal();
   $('.slider').slider({
     full_width: true,
     indicators:false,
     height:450
   });
   $('.slider').slider('pause');
   $('.tooltipped').tooltip({delay: 50});
   getMobileOperatingSystem();
});


function getMobileOperatingSystem() {
  var userAgent = navigator.userAgent || navigator.vendor || window.opera;
      // Windows Phone must come first because its UA also contains "Android"
    // if (/windows phone/i.test(userAgent)) {
    //     return "Windows Phone";
    // }

    // if (/android/i.test(userAgent)) {
    //     $( "ul .clipboard" ).addClass( "tooltipped" );
    // }

    // iOS detection from: http://stackoverflow.com/a/9039885/177710
    if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
        $( "ul .clipboard" ).removeClass( "tooltipped" );
          $( "ul .clipboard" ).addClass( "iosver" );
           $("ul .clipboard").tooltip('remove');
    }

        // $( "ul .clipboard" ).addClass( "nothing" );

    // return "unknown";
};

  // $('.modal').modal({
  //     starting_top: '4%', // Starting top style attribute
  //     ending_top: '10%', // Ending top style attribute
  //   }
  // );

  $(".button-collapse").sideNav();

    $(".slider-right").click(function() {
      $('.slider').slider('next');
    });

    $(".slider-left").click(function() {
      $('.slider').slider('prev');
    });

    $(".volunteer").click(function() {
      fbq('track', 'CompleteRegistration');
      $.ajax({
        method: "POST",
        url: "home/"+ $(this).attr('id')
      })
      $(".modal-footer-right").css('background-color','#808080');
      $(".modal-footer-right p").text('참여완료(<%= Gil.find(3).like + Plusnum.find(3).add_num + 1 %>명)');
      $(".volunteer").off();
      swal(
        '참여 완료!',
        '우주의 기운을 받으실거에요!',
        'success'
      )
      $(".num_of_peo").text(<%= @all %> + 1);
    });

    $(".finished").click(function() {
      swal(
        '이미 참가하셨습니다!',
        '친구들과 함께 참여하는건 어떠세요?',
        'error'
      )
    });

    // $("body").not( ".iosclipboard" ).click(function(){
    //   $('.iosclipboard').hide();
    // });

    $('html').click(function(e) {
       if(!$(e.target).hasClass('iosinner') )
       {
           $('.iosclipboard').hide();
       }
    });

    $(document).on('click', '.iosver', function(){
      setTimeout(function(){
        $('.iosclipboard').fadeIn("slow");
      }, 500);
    });

</script>


